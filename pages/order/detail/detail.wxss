/* //选择支付 */
.radio-group {
  display: block;
}

.radio {
  width: 100%;
  display: block;
  padding: 10rpx 0;
  font-size: 34rpx;
}

.radio_text {
  margin-left: 10rpx;
}

.disable {
  color: #999;
}

.pay_text {
  font-size: 28rpx;
  color:green;
}

.picture_list {
  padding: 20rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: flex-start;
  align-content: flex-start;
  background: #fff;
  margin-top: 5rpx;
}

.picture_item {
  margin: 10px;
  margin-left: 0;
