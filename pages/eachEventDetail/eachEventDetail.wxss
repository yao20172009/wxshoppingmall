}

.slider-container {
  overflow: hidden;
  position: relative;
}

.productionDate {
  position: absolute;
  top: 0;
  left:0;
  padding:10rpx;
  font-size: 28rpx;
  background: rgba(0, 0, 0, 0.5);
  color:#fff;
  z-index: 12
}

.swiper {
  height: 500rpx;
  width: 100%;
}

.swiper image {
  height: 100%;
  width: 100%;
}

.detail-page .info-top {
  position: relative;
}

.detail-page .info-top .start-end-wrap {
  /* font-size: 1rem; */
  white-space: nowrap;
  position: absolute;
  /* top: -1.88rem; */
  top: -67rpx;
  width: 100%;
  text-align: center;
}

.detail-page .info-top .start-end-wrap:after {
  content: '';
  display: block;
  height: 0.4rem;
  background-color: #fed931;
  width: 100%;
  position: absolute;
  bottom: 0;
}

.detail-page .info-top .table {
  height: 100rpx;
  background: #fb4521;
  color: #fff;
}

.table {
  display: table;
  width: 100%;
}

.detail-page .info-top .start-end-wrap .wings {
  height: 60rpx;
  display: inline-block;
  width: 50rpx;
}

.detail-page .info-top .start-end-wrap .start-end {
  white-space: nowrap;
  background-color: #fed931;
  color: #fb4521;
  line-height: 70rpx;
  vertical-align: top;
  display: inline-block;
  font-weight: 900;
  font-size: 0.8rem;
}

.cell_oprice {
  font-size: 0.9rem;
  line-height: 1rem;
}

.cell_oprice_line {
  font-size: 1.1rem;
  text-decoration: line-through;
}

.cell_text {
  font-size: 0.8rem;
}

.detail-page .info-top .logo {
  color: #ff0;
  font-size: 1.1rem;
  font-weight: 900;
}

.table .cell {
  display: table-cell;
  vertical-align: middle;
  text-align: center;
}

.table .cell .gift-img {
  width: 45rpx;
  height: 42rpx;
  line-height: 42rpx;
  display: inline-block;
  position: absolute;
}

.detail-page .info-top .right {
  white-space: nowrap;
  color: #ff0;
  font-weight: 900;
  width: 55%;
  text-align: left;
  line-height: 1rem;
}

.detail-page .info-top .yellow {
  font-size: 0.8rem;
  line-height: 1.1rem;
}

.detail-page .info-top .time-number {
  width: 1.3rem;
  text-align: center;
  font-size: 1rem;
  line-height: 1.1rem;
  background-color: #ff0;
  color: #fb4521;
  border-radius: 3px;
  display: inline-block;
  padding: 2px;
  margin: 1px;
}

.detail-page .info-top .day {
  font-size: 0.8rem;
  font-weight: 900;
  margin: 0 5rpx;
}

.detail-page .info-top .colon {
  font-size: 1.5rem;
  line-height: 1rem;
  vertical-align: top;
}

.detail-page .info-main {
  font-size: 1.4rem;
  color: #666;
  width: 100%;
  /* padding: 10rpx 0; */
  border-bottom: 1px solid #ddd;
}

.detail-page .info-main .title {
  text-align: left;
  padding: 10px;
  font-size: 0.8rem;
}

.detail-page .info-main .min {
  border-left: 1px solid #ddd;
  width: 20%;
  font-size: 0.8rem;
}

.detail-page .mailing-fee-text {
  text-align: center;
  padding: 24rpx;
  border-bottom: 1px solid #ddd;
}

.detail-page .mailing-fee-text text {
  color: #fff;
  font-size: 16px;
  background-color: #fb4521;
  border: 1px solid #fb4521;
  padding: 3rpx 6px;
  border-radius: 3rpx;
  margin-right: 10rpx;
}

.detail-page .winner-wrap {
  padding: 5px;
  border-bottom: 1px solid #ddd;
}

.detail-page .winner-wrap .winner {
  float: left;
  padding: 3px;
}

.detail-page .avatar {
  border: 1px solid #eee;
  /* margin-right: 5rpx; */
  width: 85rpx;
  height: 85rpx;
  vertical-align: middle;
}

.detail-page .winner-wrap .winner-list:after {
  content: '';
  display: table;
  clear: both;
}

.detail-page .winner-wrap .share-summary {
  color: #fb4521;
}

.detail-page .winner-wrap .seed-count {
  margin: 0 3px;
  font-size: 16px;
  font-weight: 700;
}

.detail-page .shop {
  text-decoration: none;
  display: block;
  color: #555;
  border-bottom: 1px solid #ddd;
  position: relative;
  padding-top:10rpx;
}

.winner-wrap .rmb {
  font-size: 40rpx;
  color:red;
  font-weight: bold;
}

.winner-wrap .newPrice{
  font-size: 70rpx;
  color:red;
  font-weight: bold;
}

.winner-wrap .oldPrice{
  font-size: 40rpx;
  color:#999;
  text-decoration:line-through;
  margin-left: 20rpx;
}

.detail-page .shop text {
  position: absolute;
  font-size: 32rpx;
  top:22%;
}

.detail-page .shop .avatar1 {
  margin: 10rpx;
  width:50rpx;
  height:50rpx;
}

.action-bars {
  position: absolute;
  bottom: 0;
  display: table;
  width: 100%;
  left: 0;
  height: 100rpx;
  z-index: 99;
  border-radius: 0rpx;
}

.status-bar .btn {
  padding: 0;
  height: 100rpx;
  font-size: 1.7rem;
  border-radius: 0rpx;
  line-height: 100rpx;
  text-align: center;
  z-index: 3;
  border: 1rpx transparent solid;
}

.status-bar {
  color: #fff;
  padding: 6px 10px;
  border-radius: 0rpx;
  -webkit-transition: all 1s;
  transition: all 1s;
  display: table-cell;
  background-color: #f60;
  white-space: nowrap;
}

button {
  border-radius: 0rpx;
}
/* sku选择 */

.dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 20;
  background: rgba(0, 0, 0, 0.7);
  display: none;
}

.dialog__container {
  position: fixed;
  bottom: 0;
  width: 750rpx;
  background: white;
  transform: translateY(150%);
  transition: all 0.4s ease;
  z-index: 101;
}

.dialog--show .dialog__container {
  transform: translateY(0);
}

.dialog--show .dialog__mask {
  display: block;
  position: absolute;
}

.image-sku {
  width: 200rpx;
  height: 200rpx;
  z-index: 12;
  position: absolute;
  left: 20px;
  top: -12px;
  border-radius: 20rpx;
}

.image-close {
  width: 60rpx;
  height: 60rpx;
  position: fixed;
  right: 10rpx;
  top: 0rpx;
}

.column {
  width: 66%;
  height: 100%;
  margin-left: 34%;
  flex-direction: column;
  margin-top: 10rpx;
}

.row {
  display: flex;
  justify-content: flex-start;
  width: 100%;
  height: 100%;
  align-items: center;
  padding-top: 30rpx;
  margin-bottom: 10rpx;
}

.border-line {
  width: 100%;
  height: 2rpx;
  display: inline-block;
  margin: 25rpx 0rpx;
  background-color: gainsboro;
  text-align: center;
}

.sku-title {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.sku-price {
  color: red;
  margin: 1rpx;
}

.row .quantity-position {
  position: absolute;
  right: 30rpx;
}

.button_box {
  width: 100%;
  height: 120rpx;
}

.button-green {
  background: green;
  width: 50%;
  height: 100%;
  line-height: 120rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: white;
  text-align: center;
  display: inline-block;
  border-radius: 0rpx;
}

.button-red {
  width: 100%;
  height: 100%;
  line-height: 120rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: white;
  text-align: center;
  display: inline-block;
  border-radius: 0rpx;
}

/*主容器*/

.stepper {
  width: 284rpx;
  height: 80rpx;
  /*给主容器设一个边框*/
  border: 1px solid #ccc;
  border-radius: 3px;
  margin: 0 auto;
}

.stepper-total {
  /* width: 284rpx; */
  margin: 0 auto;
  text-align: right;
  color: red;
}

.buyNum {
  padding-left: 20rpx;
}

/*加号和减号*/

.stepper text {
  width: 80rpx;
  line-height: 80rpx;
  text-align: center;
  float: left;
}

/*数值*/

.stepper input {
  width: 120rpx;
  height: 80rpx;
  float: left;
  margin: 0 auto;
  text-align: center;
  font-size: 34rpx;
  /*给中间的input设置左右边框即可*/
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
}

/*普通样式*/

.stepper .normal {
  color: black;
}

/*禁用样式*/

.stepper .disabled {
  color: #ccc;
}

/* 地址 */

.Dbox {
  width: 100%;
  background-color: #fff;
  border-bottom: 1px solid #ddd;
  /* margin-top: 20rpx; *//* border-top: 1rpx solid #999; */
}

.Dbox .address {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 0;
}

.address .landmark {
  width: 15%;
  padding-left: 14rpx;
}

.landmark image {
  width: 100%;
  height: 125rpx;
}

.address .detailed-address {
  width: 65%;
  display: inline-block;
  padding-left: 15rpx;
}

.detailed-address .address-box {
  flex-direction: column;
}

.address .select-address {
  width: 13%;
  /* padding-top: 35rpx; */
  margin-right: 5rpx;
}

.select-address image {
  height: 50rpx;
  width: 50%;
}

.detailed-address .name-tel {
  margin-bottom: 10rpx;
  font-weight: 700;
  font-size: 32rpx;
}

.detailed-address .name-tel-no {
  font-size: 32rpx;
  margin-top: 10rpx;
}

.detailed-address .address-box {
  font-size: 28rpx;
  color: #888;
  line-height: 36rpx;
  overflow: hidden;
}

.detailed-address .address-box .box-add {
  margin-bottom: 5rpx;
}

/* 错误提示框 */

.errors {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: -99;
}

.errors_index {
  z-index: 999;
}

.errors_box {
  position: fixed;
  top: 30%;
  left: 5%;
  width: 90%;
  height: 20%;
  /* display: flex; */
  text-align: center;
  align-items: center;
  border-radius: 5px;
  background: #999;
  font-size: 40rpx;
}

.errors_text {
  color: red;
}

.errors_mask {
  width: 100%;
  height: 100%;
}

.share{
  width: 100rpx;
  height: 100rpx;
  padding:0 20rpx;
  position: fixed;
  right: 0;
  /* top: 0; */
  bottom: 320rpx;
  margin: auto;
  background-size: 100rpx 100rpx;
  background-repeat:no-repeat;
  border:none;
  z-index: 100;
}

button[plain] {
  border:none;
}

.imgShare {
  z-index: 99;
  padding:0;
}