}

.errText {
  position: absolute;
  bottom: 100rpx;
  width: 100%;
  z-index: 99;
  text-align: center;
}

.errText text {
  background: #fed931;
  color: red;
  padding: 10rpx;
}

.detail-page .mailing-fee {
  position: absolute;
  bottom: 40px;
  right: 10px;
  width: 25%;
  height: 45%;
  z-index: 3;
}

.detail-page .mailing-fee-box {
  position: absolute;
  bottom: 80rpx;
  right: 20rpx;
  width: 25%;
  height: 35%;
  z-index: 3;
  background: red;
