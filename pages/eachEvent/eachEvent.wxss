page {
  font-size: 1.4rem;
  margin: 0;
  height: 100%;
  background-color: #ED1B38;
}

.page-container {
  overflow: hidden;
}

.page-container .list-page-container {
  position: relative;
  height: 100%;
  overflow: auto;
}

.page-container .list-page {
  background-color: #ED1B38;
  /* 双11背景色 *//* padding-bottom: 100px; */
  transition: top 1.5s ease-in-out 0s;
  -webkit-transition: top 1.5s ease-in-out 0s;
  position: relative;
  z-index: 3;
  top: 0;
}

.page-container .list-page .date-time {
  position: fixed;
  right: 0rpx;
  padding: 6rpx 10rpx;
  top: 0rpx;
  font-size: 0.8rem;
  background: rgba(0, 0, 0, 0.6);
  color: #fff;
  border-top-left-radius: 5rpx;
  border-bottom-left-radius: 5rpx;
  z-index: 999;
}

/* //活动名称 */
.page-container .list-page .date-name {
  position: absolute;
  font-size: 32rpx;
  color:#fff;
  font-weight: 900;
  top:400rpx;
  z-index: 999;
  width: 100%;
  text-align: center;
}

.page-container .list-page .list-poster {
  width: 100%;
  display: block;
  height: 510rpx;
}

.list {
  padding: 10px;
}

.list .next-date {
  margin: 20rpx auto;
  text-align: center;
  position: relative;
}

.list .next-date .benqi {
  text-align: left;
  position: absolute;
  left: 0;
  bottom: 6rpx;
  font-weight: bold;
  font-size: 0.9rem;
  padding: 20rpx 20rpx 0 20rpx;
}

.list .next-date .heise {
  color: #fff;
}

.list .next-date .huise {
  color: rgba(0, 0, 0, 0.7);
}

.list .next-date .next-time {
  text-align: center;
  position: absolute;
  right: 0;
  bottom: 6rpx;
  font-weight: bold;
  font-size: 0.9rem;
  padding: 20rpx 20rpx 0 20rpx;
}

.next-left, .next-right {
  width:80rpx;
  height:80rpx;
  position: absolute;
}

.next-left {
  left: 0;
  bottom: 0;
}

.next-right {
  right:0;
  bottom:0
}

.eventList_no {
  color:#fff;
  font-size: 37rpx;
  width: 82%;
  margin:0 auto;
  text-align: left;
}



.event_text {
  text-align: center;
  font-size: 0.9rem;
  margin-bottom: 20rpx;
  color: red;
  font-weight: bold;
}

.item {
  position: relative;
  margin-bottom: 10px;
  background-color: #fcefda;
  overflow: hidden;
}

.item .itemItem {
  color: #666;
  display: block;
  padding: 20rpx;
}

.item .left {
  padding: 0;
  width: 40%;
  position: relative;
}

.item .left .mailing-fee {
  position: absolute;
  bottom: 15px;
  right: 15px;
  width: 33%;
  height: 33%;
}

.item .left .avatar {
  width: 100%;
  display: block;
  height: 267rpx;
  border: 1px solid #e4ba66;
}

.item .right {
  top: 0;
  left: 40%;
  right: 0;
  padding: 10px;
  height: 267rpx;
  position: absolute;
}

.item .right .boughtQuantity {
  position: absolute;
  top: 4rpx;
  right: 4rpx;
  font-size: 0.6rem;
  padding: 5rpx;
  background: red;
  color: #fff;
}

.item .right .title {
  margin: 0;
  font-size: 0.8rem;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  font-weight: bold;
}

.item .left .kindDesc {
  position: absolute;
  right: -1rpx;
  top: 22rpx;
  margin: 0;
  font-size: 0.8rem;
  background: rgba(0, 0, 0, 0.7);
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.item .left .kindDesc .kindDescText {
  color: #fff;
  padding: 10rpx 10rpx;
}

.item .right .price {
  color: #f00;
  font-size: 1.2rem;
  font-weight: 900;
  margin-right: 1rem;
}

.item .right .ori_price {
  color: #999;
  font-size: 0.9rem;
  margin-right: 1rem;
  text-decoration: line-through;
}

.quehuo {
  display: block;
  width:150rpx;
  height:50rpx;
  font-size: 32rpx;
  background: #646464;
  color:#fff;
  font-weight: 900;
  text-align: center;
  border-radius: 1rpx;
}

.item .right .ori_oprice {
  text-decoration: line-through;
}

.item .item-status {
  position: relative;
  margin-top: 5rpx;
}

.item .item-status .status-bar {
  display: inline-block;
}

.item .item-sales {
  font-size: 0.8rem;
  color: #999;
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
}

.item .boughtQuantity2 {
  display: block;
  font-size: 0.8rem;
  color: #999;
  margin-top: 5rpx;
}

.item1 .boughtQuantity1 {
  display: block;
  font-size: 0.5rem;
  background: #F8188C;
  color: #fff;
  position: absolute;
  top:0rpx;
  right: 0rpx;
  padding:5rpx;
}

.item1 .sales1 {
  font-size: 0.65rem;
  color: #999;
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
}

.status-bar {
  color: #fff;
  padding: 6px 10px;
  border-radius: 5px;
  -webkit-transition: all 1s;
  transition: all 1s;
  display: table-cell;
  background-color: #f60;
  white-space: nowrap;
  font-size: 0.8rem;
}

.status-bar.started {
  background-color: #fb4521;
}

.item .top_shadow {
  position: absolute;
  top: -4rpx;
  left: 0;
  height: 8rpx;
  width: 100%;
  background-repeat: repeat-x;
}

.item .bottom_shadow {
  position: absolute;
  bottom: -4rpx;
  left: 0;
  height: 8rpx;
  width: 100%;
  background-repeat: repeat-x;
}

.item .right_shadow {
  position: absolute;
  right: -2px;
  top: 0;
  background-repeat: repeat-y;
  bottom: 0;
  width: 8rpx;
  height: 300rpx;
}

.item .left_shadow {
  position: absolute;
  left: -2px;
  top: 0;
  background-repeat: repeat-y;
  bottom: 0;
  width: 8rpx;
  height: 300rpx;
}

.noMore {
  display: block;
  width: 100%;
  font-size: 0.8rem;
  margin: 50rpx 0;
  text-align: center;
  color: #999;
}

