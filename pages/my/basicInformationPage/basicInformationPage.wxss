.lineview {
  width: 100%;
  height: 1px;
  background: rgba(0,0,0,0.1);
}

page {
  background: #f5f5f5;
}

.containers {
  display: flex;
  flex-direction: column;
}

.firstlayout {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 350rpx;
  justify-content: center;
  margin-top: -60rpx;
  background: #fff;
  margin-bottom: 30rpx;
}

.firstimagestyle {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}

.thirdlayout {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: #fff;
}

.thirdlayoutitem {
  width: 20%;
  height: 100rpx;
  /*弹性模型*/
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-left: 40rpx;
}

.secondimagestyle {
  width: 40rpx;
  height: 40rpx;
  margin: 20rpx;
}

.fourthlayout {
  display: flex;
  flex-direction: row;
  width: 80%;
  height: 100%;
}

.fourthlayoutitem {
  flex: 1;
  height: 100%;
  text-align: right;
}

.fourthlayoutitemtwo {
  display: flex;
  flex-direction: row;
  width: 50rpx;
  height: 100%;
  align-items: center;
}

.thirdimagestyle {
  width: 60rpx;
  height: 60rpx;
  margin: 20rpx;
}

.margin {
  margin-top: 24rpx;
}
