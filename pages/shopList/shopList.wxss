@import "../category/category.wxss";

.containers1 {
  background: #e9e9e9;
}

.yuanbao {
  width:40rpx;
  height: 40rpx;
  margin-left:20rpx;
}

.noHang {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

.header-right {
  width: 13%;
}

.header-right-title {
  color: #000;
  display: flex;
  align-items: center;
  height: 100%;
  font-size: 30rpx;
}

.content-header1{
  height: 180rpx;
  width: 100%;
  background: #fff;
  display: flex;
  flex-direction: row;
  margin-bottom: 10rpx;
}

.content-header2 {
  height: 180rpx;
  width: 100%;
  background: #fff;
  display: flex;
  flex-direction: row;
  margin-bottom: 10rpx;
}


.content-header2:active{
  background: rgba(0, 0, 0, 0.4);
}


.header-center, .headerText, .huiseText {
  color: #000;
}

.yuanbao_jifen {
  margin-left:10rpx;
  font-size: 32rpx; 
  color:#B8860B;
}

.yuanbao_lv {
  font-size: 32rpx; 
  color:#E4393C;
}

.huiseText {
  display: flex;
  align-items: center;
}

.header-center {
  width: 66%;
  padding-top: 0;
}

.header-left .header-left-yuanjiao image {
  width: 100rpx;
  height: 100rpx;
}

.header-left {
  width: 21%;
  display: flex;
  align-items: center;
}

.header-left-yuanjiao {
  height: 100rpx;
  width: 100rpx;
  border: 4rpx solid #ddd;
  margin: 0 0 0 24rpx;
}

.center_top, .center_bottom {
  text-align: center;
  padding: 50rpx;
  color: #aaa;
  background: #fff;
}
