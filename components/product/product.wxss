.clear {
  clear: both;
  overflow: hidden;
}

navigator {
  display: inline;
}

.list {
  margin-top: 10px;
}

.list .list_item {
  margin-top: 10px;
  padding: 10px;
  height: 100px;
  border-bottom: 1px solid #e8e8e8;
}

.list .list_item .img {
  float: left;
  width: 35%;
  height: 100%;
}

.list .list_item .img image {
  width: 100%;
  height: 100%;
}

.list .list_item .info {
  width: 64%;
  float: right;
  height: 100px;
  position: relative;
}

.list .list_item .info .title {
  color: #333;
  margin-left: 10px;
  font-size: 15px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  line-height: 1.3;
  height: 36px;
}

.list .list_item .info .price {
  color: #f62e5e;
  margin-left: 10px;
  /* margin-top:10px; */
  font-size: 16px;
  font-weight: bold;
}

.list .list_item .info .num {
  /* position: absolute;
  left:0px;
  bottom:10px; */
  color: #747474;
  margin-left: 10px;
  font-size: 13px;
}

.list .list_item .info  .supplier {
  color: #747474;
  margin-left: 10px;
  font-size: 13px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

/* //2 */

.itemContainer {
  background-color: transparent;
}

.child {
  width: 46%;
  margin: 1%;
  display: inline-block;
  vertical-align: top;
  background-color: #fff;
  padding: 1%;
}

.child view {
  display: flex;
  flex-direction: column;
  margin: 10rpx;
}

.child view.money {
  display: block;
  margin-left: 0;
  margin:1rpx 0;
}

.child-img {
  height: 300rpx;
  width: 100%;
}

.moneyIcon {
  font-size: 30rpx;
  color: #f8188c;
}

.child view.money .moneyText {
  color: #f8188c;
}

.itemTitle {
  font-size: 30rpx;
  margin-top: 10rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  height: 72rpx;
  line-height: 1.2;
}

.money .salesMoney {
  display: inline-block;
  text-decoration: line-through;
  font-size: 30rpx;
  color: #999;
}

.supplierName {
  font-size: 28rpx;
  color: #999;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
